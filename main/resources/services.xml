<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="catalogManager" class="com.goldenpond.stampbook.biz.CatalogManager" factory-method="getInstance"></bean>
	<bean id="collectionService" class="com.goldenpond.stampbook.biz.CollectionService" scope="singleton">
		<property name="userCollectionDao" ref="userCollectionDao"></property>
	</bean>

</beans>
